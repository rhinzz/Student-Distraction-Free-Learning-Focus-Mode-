{
  "name": "focusmode-student-learning",
  "version": "1.0.0",
  "description": "Aplikasi FocusMode untuk membantu siswa belajar tanpa gangguan",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "api": "node start-app.js",
    "dev": "concurrently \"npm run api\" \"npm run serve\"",
    "serve": "webpack serve --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "setup-db": "node setup-database.js",
    "setup": "npm run setup-db",
    "test": "echo \"No tests specified\"",
    "clean": "rm -rf dist",
    "clean:all": "rm -rf dist node_modules package-lock.json",
    "install:clean": "npm run clean:all && npm install",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,json,md}\"",
    "fullstack": "concurrently --names \"API,CLIENT\" \"npm run api\" \"npm run serve\"",
    "api:only": "node start-app.js",
    "client:only": "webpack serve --config webpack.dev.js",
    "production": "npm run build && npm run api",
    "heroku-postbuild": "npm run build"
  },
  "keywords": [
    "pwa",
    "focus",
    "study",
    "productivity",
    "education",
    "student",
    "learning"
  ],
  "author": "Student Developer",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "babel-loader": "^9.1.0",
    "concurrently": "^9.2.1",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^6.8.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.0",
    "prettier": "^2.8.0",
    "style-loader": "^3.3.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0",
    "webpack-dev-server": "^4.15.0",
    "webpack-merge": "^5.9.0",
    "workbox-webpack-plugin": "^7.3.0"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "helmet": "^7.0.0",
    "idb": "^7.1.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "web-push": "^3.6.7"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ceciuww/Student-Distraction-Free-Learning-Focus-Mode-.git"
  },
  "bugs": {
    "url": "https://github.com/ceciuww/Student-Distraction-Free-Learning-Focus-Mode-.git"
  },
  "homepage": "https://github.com/ceciuww/Student-Distraction-Free-Learning-Focus-Mode-.git"
}
